<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="client.PanelController">
    <HBox>
        <TextField fx:id="fileActive" promptText="File" HBox.hgrow="ALWAYS"/>
        <TextField fx:id="filePath" promptText="File path" HBox.hgrow="ALWAYS"/>
        <Button onAction="#upToDirectory" text="Up"/>
    </HBox>
    <TableView fx:id="tableInfo" VBox.vgrow="ALWAYS"/>
    <properties>
        <ctrl>
            <fx:reference source="controller"/>
        </ctrl>
    </properties>
</VBox>
