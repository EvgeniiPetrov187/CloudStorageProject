<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="client.PanelController">
    <HBox>
        <TextField fx:id="filePath" promptText="File path" HBox.hgrow="ALWAYS" editable="false"/>
        <Button onAction="#upToDirectory" text="Up" minWidth="70"/>
    </HBox>
    <TableView fx:id="tableInfo" VBox.vgrow="ALWAYS"/>
    <properties>
        <ctrl>
            <fx:reference source="controller"/>
        </ctrl>
    </properties>
</VBox>
